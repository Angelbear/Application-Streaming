package $1;

import android.content.Intent;
import android.os.Bundle;

public class $2 extends android.app.Activity {
    
    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        Intent i = new Intent("cn.edu.tsinghua.hpc.LOADING_REQUEST");
        i.putExtra("package", this.getPackageName());
        sendBroadcast(i);
        finish();
    }

}

